<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>CMS</title>
    <link rel="stylesheet" href="https://unpkg.com/@decapcms/decap-cms@3.1.9/dist/decap-cms.css" />
  </head>
  <body>
    <script src="https://unpkg.com/@decapcms/decap-cms@3.1.9/dist/decap-cms.js"></script>
    <script>
      // Tell Decap/Netlify CMS to read your config
      window.CMS_MANUAL_INIT = true;
      CMS.init({ config: '/admin/config.yml' });
    </script>
  </body>
</html>
